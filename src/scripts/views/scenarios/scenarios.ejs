<a class="logo" href="//forio.com" target="_blank"> <img src="styles/assets/logo.svg" alt="/forio"> </a>
<h1> Optimize Your  <%= selected %>
    <div class="scen-selector">
        <select id="lstScenarioSwitcher">
            <% _(scenarios).each(function(name, url){ %>
                <option value="<%= url %>" <% if(url === selected) { %> selected <% } %> > <%= selected %> <%= name %> </option>
            <% }) %>
        </select>
    </div>
    with Julia and Forio Epicenter
</h1>
